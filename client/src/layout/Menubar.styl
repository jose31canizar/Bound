.menu-bar
  transition: 0.5s ease
  display: flex
  flex-direction: column
  padding: 20px
  padding-left: 25px
  height: 100vh
  box-sizing: border-box
  width: 300px
  position: absolute
  top: 0
  border-right: 2px solid black
  background: white
  label
    display: inline-block
  a
    margin: 10px 0